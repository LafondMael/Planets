maelunix@DESKTOP-L5OHQ54:~/planets/real$ cd dwarf-planets
maelunix@DESKTOP-L5OHQ54:~/planets/real/dwarf-planets$ rm pluto.jpeg
maelunix@DESKTOP-L5OHQ54:~/planets/real/dwarf-planets$ cd ..
maelunix@DESKTOP-L5OHQ54:~/planets/real$ rmdir dwarf-planets
maelunix@DESKTOP-L5OHQ54:~/planets/real$ cd ..
maelunix@DESKTOP-L5OHQ54:~/planets$ cd ..
